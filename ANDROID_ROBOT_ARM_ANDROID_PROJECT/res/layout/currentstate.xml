           android:layout_weight = "0.5" />             
    
  </LinearLayout> 
  
   <!--  LAYOUT SPACE 4 -->
   <LinearLayout
       android:id="@+id/layoutSpace08"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content">
       
       <TextView 
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:gravity="center_horizontal" 
           android:text = "       "/>
   
   </LinearLayout>
  
  <!-- HAND BUTTONS --> 
   <LinearLayout
       android:id="@+id/layout09"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content">
       
      <TextView
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:layout_weight = "0.5"
           android:text = "Hand Motor" />
       <TextView 
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:layout_weight = "0.5"
