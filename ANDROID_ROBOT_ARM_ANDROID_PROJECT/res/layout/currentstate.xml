  <!-- WRIST BUTTONS -->  
  <LinearLayout
       android:id="@+id/layout07"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content">
       
      <TextView
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:layout_weight = "0.5"
           android:text = "Wrist Motor" />
       <TextView 
           android:layout_width="fill_parent"
           android:layout_height="wrap_content"
           android:layout_weight = "0.5"
